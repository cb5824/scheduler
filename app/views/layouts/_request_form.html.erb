<div class="form_block_container">
  <div class="title_card">
  <p>Right To Work Request Form</p>
  <% if @errors %>
    <div>
      <ul>
        <% @errors.each do |error_text| %>
          <li><%= error_text %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
  </div>
  <div class="form_block" id="statement">
    <div>
      <p>Please ensure that all associated SSWP’s (Site Specific Work Plans) and ECN’s (Engineering Change Notices) have been approved prior to submitting an RTW (Right To Work) Request.  RTW Requests must be submitted at least three weeks prior to the intended start of work.  Request Submittals are not complete and will not be considered for approval until all necessary scheduling information has been included on this form.</p>
    </div>
  </div>

  <div class="form_header">Requester Information</div>
  <div class="form_block">
    <div class="form_block_line">
    <%= f.label :requestor_name, "Name: " %>
    <%= f.text_field :requestor_name, class: "grow" %>
    </div>
    <br>
    <div class="form_block_line">
    <%= f.label :requestor_email, "Email: " %>
    <%= f.email_field :requestor_email, class: "grow"  %>
    <%= f.label :requestor_phone, "Phone: " %>
    <%= f.text_field :requestor_phone, class: "grow"  %>
    </div>
    <br>
    <div class="form_block_line">
    <%= f.label :contractor, "Organization/Company:" %>
    <%= f.text_field :contractor, class: "grow"  %>
    </div>
    <br>
    <div class="form_block_line">
  <%= f.label :requestor_project, "Project: " %>
    <%= f.text_field :requestor_project, class: "grow"  %>
    <%= f.label :requestor_work_directive, "Work Directive: " %>
    <%= f.text_field :requestor_work_directive, class: "grow"  %>
    </div>
    <br>
    <div class="form_block_line">
    <%= f.label :color, "Color:" %>
    <%= f.select(:color, @color_options.map { |value| [ value[0], value[1], {class: value[1]} ] }) %>
    </div>
  </div>

  <div class="form_header">Time Frame</div>
  <div class="form_block">
    <div class="form_block_line">

    <%= f.label :year, "Year:" %>
    <%= f.number_field :year, min: 2020 %>

    <%= f.label :week, "Week:" %>
    <%= f.number_field :week, min: 1 %>
    <%= f.label :night_work, "Night Work?:" %>
    <%= f.check_box :night_work, {}, checked_value = 1, unchecked_value = 0 %>
    </div>
    <div class="form_block_line">

    </div>
</div>

    <div class="form_header">Description of Work</div>
    <div class="form_block">
      <div class="form_block_line">
        <div class="form_block_line_item">
          <%= f.label :title, "Job/Task Title:" %>
          <%= f.text_field :title, class: "grow"  %>
        </div>
    </div>
      <br>
      <div class="form_block_line">
        <div class="form_block_line_item">
            <%= f.label :description, "Description of Work:" %>
            <%= f.text_area :description, class: "tall" %>
        </div>
      </div>
      <br>
      <div class="form_block_line">
        <div class="form_block_line_item">

      <%= f.label :sswps, "Associated (approved) SSWP's" %>
      <%= f.text_area :sswps, class: "tall"  %>
        </div>
      <div class="form_block_line_item">
      <%= f.label :change_notices, "Associated Change Notice(s)" %>
      <%= f.text_area :change_notices, class: "tall"  %>
    </div>
      <br>
    </div>

    </div>
    <div class="form_header">Additional Information</div>
    <div class="form_block">
      <div class="form_block_line left_align">
      <%= f.radio_button :rwp, "yes" %> Yes
      <%= f.radio_button :rwp, "no" %> No
      <%= f.label :rwp, "All crew members will have curent RWP certification at time of project." %>
      </div>
      <h4 class="leftmargin bottom_margin">Will this work...</h4>
      <div class="form_block_line left_align">
      <%= f.radio_button :ocs, "yes" %> Yes
      <%= f.radio_button :ocs, "no" %> No
      <%= f.label :ocs, "Require OCS or other high voltage equipment to be de-energized?" %>
      </div>
      <div class="form_block_line left_align">
      <%= f.radio_button :disturb, "yes" %> Yes
      <%= f.radio_button :disturb, "no" %> No
      <%= f.label :disturb, "Disturb the track ballast, ties, or structure?" %>
      </div>
      <div class="form_block_line left_align">
      <%= f.radio_button :rrm, "yes" %> Yes
      <%= f.radio_button :rrm, "no" %> No
      <%= f.label :rrm, "Require Railroad Maintenance Machinery (RRM) or Hirail equipment on track?" %>
      </div>
      <div class="form_block_line left_align">
      <%= f.radio_button :foul, "yes" %> Yes
      <%= f.radio_button :foul, "no" %> No
      <%= f.label :foul, "Require equipment off track that may foul track?" %>
      </div>
      <div class="form_block_line left_align">
      <%= f.radio_button :crossings, "yes" %> Yes
      <%= f.radio_button :crossings, "no" %> No
      <%= f.label :crossings, "Disable/Affect crossings?" %>
      </div>
      <div class="form_block_line left_align">
      <%= f.radio_button :underground, "yes" %> Yes
      <%= f.radio_button :underground, "no" %> No
      <%= f.label :underground, "Involve underground work, requiring utility locates?" %>
      </div>
      <div class="form_block_line left_align">
      <%= f.radio_button :flagging, "yes" %> Yes
      <%= f.radio_button :flagging, "no" %> No
      <%= f.label :flagging, "Require traffic control?" %>
      </div>
    </div>


    <div class="form_header">Daily Schedule</div>
    <div class="form_block">
      <div class="form_block_line">
        <div class="form_block_spaced">Mon<%= f.check_box :mon, {}, checked_value = 1, unchecked_value = 0 %></div>
        <div class="form_block_spaced">Tues<%= f.check_box :tue, {}, checked_value = 1, unchecked_value = 0 %></div>
        <div class="form_block_spaced">Wed<%= f.check_box :wed, {}, checked_value = 1, unchecked_value = 0 %></div>
        <div class="form_block_spaced">Thurs<%= f.check_box :thu, {}, checked_value = 1, unchecked_value = 0 %></div>
        <div class="form_block_spaced">Fri<%= f.check_box :fri, {}, checked_value = 1, unchecked_value = 0 %></div>
        <div class="form_block_spaced">Sat<%= f.check_box :sat, {}, checked_value = 1, unchecked_value = 0 %></div>
        <div class="form_block_spaced">Sun<%= f.check_box :sun, {}, checked_value = 1, unchecked_value = 0 %></div>
      </div>
      <% if @action == "edit" %>
        <div class="form_block_line">
