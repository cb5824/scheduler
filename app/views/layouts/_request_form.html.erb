                    <%= p.select(:worker_secondary3, @worker_options.map { |value| [ value[0], value[1] ] }, {}, {class: "secondary_dropdown"}) %>
                  </div>
                  <div>
                    <%= p.select(:worker_secondary4, @worker_options.map { |value| [ value[0], value[1] ] }, {}, {class: "secondary_dropdown"}) %>
                    <%= p.select(:worker_secondary5, @worker_options.map { |value| [ value[0], value[1] ] }, {}, {class: "secondary_dropdown"}) %>
